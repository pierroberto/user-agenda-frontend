{
  "name": "React-Boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "NODE_CONFIG_DIR=./dist/config NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server node dist/server",
    "start:dev": "NODE_CONFIG_DIR=./src/config nodemon .",
    "webpack": "NODE_CONFIG_DIR=./src/config NODE_PATH=NODE_PATH:./src/shared:./src/server webpack-dev-server --no-info --config webpack.config.js",
    "build:webpack": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server webpack --config webpack.config.production.js --watch --progress --profile --color -p",
    "build:node": "babel -s inline -D src/ -d dist",
    "build": "npm run build:node && npm run build:webpack",
    "lint": "NODE_PATH=NODE_PATH:./src/shared:./src/server  eslint src"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "debug": "*",
    "es6-promise": "^4.2.2",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "form-data": "^2.3.1",
    "helmet": "^2.1.1",
    "intl": "^1.2.4",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^2.5.3",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.1.0",
    "react-fontawesome": "^1.6.1",
    "react-intl": "^2.1.3",
    "react-notification-system": "^0.2.10",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.1",
    "reactstrap": "^4.8.0",
    "redux": "^3.5.2",
    "sass-loader": "^4.0.0",
    "serve-favicon": "*",
    "source-map-support": "*",
    "uuid": "^3.1.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-loader": "*",
    "babel-plugin-syntax-object-rest-spread": "*",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "*",
    "babel-preset-stage-0": "*",
    "chai": "*",
    "css-loader": "*",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.6",
    "node-sass": "*",
    "style-loader": "*",
    "toolbox-loader": "*",
    "webpack-dev-server": "^1.9.0"
  }
}
